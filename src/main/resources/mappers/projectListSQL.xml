<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="data.project.ProjectListMapper">
	<select id="getTotalCount" resultType="int">
		select count(*) from project
	</select>
	<select id="getAllProjects" resultType="pdto">
		select * from project order by idx asc limit 8
	</select>	
	<select id="getCateData" parameterType="String" resultType="pdto">
		select * from project where category=#{category}
	</select>
</mapper>